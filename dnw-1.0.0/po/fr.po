#: src/dnw.c:105
#, c-format
msgid "Usage: dnw [options] file\n"
msgstr "Utilisation: dnw [options] fichier\n"

#: src/dnw.c:106
#, c-format
msgid "Options:\n"
msgstr "Options:\n"

#: src/dnw.c:107
#, c-format
msgid ""
" -a, -A, --address=ADDRESS              Specify the download ADDRESS "
"(default is 0xc0008000)\n"
msgstr ""
" -a, -A, --address=ADDRESSE             Spécifie l'ADRESSE de téléchargement "
"(le défaut est 0xc0008000)\n"

#: src/dnw.c:108
#, c-format
msgid ""
" -b, -B, --block-size=SIZE              Specify the USB transfer block SIZE "
"(default is 512)\n"
msgstr ""
" -b, -B, --block-size=TAILLE            Spécifie la TAILLE de bloc pour le "
"transfert USB (le défaut est 512)\n"

#: src/dnw.c:109
#, c-format
msgid ""
" -c, -C, --config=VALUE                 Specify the USB configuration VALUE "
"(default is 1)\n"
msgstr ""
" -c, -C, --config=VALEUR                Spécifie la VALEUR de la "
"configuration USB (le défaut est 1)\n"

#: src/dnw.c:110
#, c-format
msgid ""
" -d, -D, --device=VENDOR:PRODUCT        Specify the USB VENDOR:PRODUCT "
"device ID (default is \"04E8:1234\")\n"
msgstr ""
" -d, -D, --device=VENDEUR:PRODUIT       Spécifie l'identifiant VENDEUR:"
"PRODUIT USB (le défaut est \"04E8:1234\")\n"

#: src/dnw.c:111
#, c-format
msgid ""
" -e, -E, --endpoint=ADDRESS             Specify the USB transfer endpoint "
"ADDRESS (default is 2)\n"
msgstr ""
" -e, -E, --endpoint=ADDRESSE            Spécifie l'ADDRESSE du point d'accès "
"USB (le défaut est 2)\n"

#: src/dnw.c:112
#, c-format
msgid " -h, -H, --help                         Print the option help\n"
msgstr " -h, -H, --help                         Affiche l'aide des options\n"

#: src/dnw.c:113
#, c-format
msgid ""
" -i, -I, --interface=NUMBER             Specify the USB interface number "
"(default is 0)\n"
msgstr ""
" -i, -I, --interface=NUMERO             Spécifie le NUMERO de l'interface "
"USB (le défaut est 0)\n"

#: src/dnw.c:114
#, c-format
msgid " -q, -Q, --quiet                        Turn off display messages\n"
msgstr ""
" -q, -Q, --quiet                        Désactive l'affichage des messages\n"

#: src/dnw.c:115
#, c-format
msgid ""
" -t, -T, --timeout=DURATION             Specify the USB transfer timemout "
"DURATION in ms (default is 3000)\n"
msgstr ""
" -t, -T, --timeout=DUREE                Spécifie la DUREE des délais pour le "
"transfert USB en ms (le défaut est 3000)\n"

#: src/dnw.c:116
#, c-format
msgid " -v, -V, --version                      Print the version information\n"
msgstr ""
" -v, -V, --version                      Affiche les informations de version\n"

#: src/dnw.c:124
#, c-format
msgid "USB bulk-transfer utility for Samsung devices.\n"
msgstr "Utilitaire de transfert bulk USB pour les appareils Samsung.\n"

#: src/dnw.c:125
#, c-format
msgid "Copyright (C) 2011, Michel Stempin <michel.stempin@wanadoo.fr>\n"
msgstr "Copyright (C) 2011, Michel Stempin <michel.stempin@wanadoo.fr>\n"

#: src/dnw.c:126
#, c-format
msgid ""
"License GPL2+: GNU GPL version 2 or later <http://www.gnu.org/licenses/gpl-"
"2.0.html>.\n"
msgstr ""
"License GPL2+: GNU GPL version 2 ou ultérieure <http://www.gnu.org/licenses/"
"gpl-2.0.html>.\n"

#: src/dnw.c:127
#, c-format
msgid "This is free software: you are free to change and redistribute it.\n"
msgstr ""
"Ceci est un logiciel libre, vous êtes libre de le modifier et de le "
"redistribuer.\n"

#: src/dnw.c:128
#, c-format
msgid "There is NO WARANTY, to the extent permitted by law.\n"
msgstr ""
"Ce logiciel n'est accompagné d'ABSOLUMENT AUCUNE GARANTIE, dans les limites "
"autorisées par la loi applicable.\n"

